[submodule "neovim/.config/nvim"]
	path = neovim/.config/nvim
	url = git@github.com:elanmed/neovim-config.git
	; url = https://github.com/elanmed/neovim-config.git
[submodule "base16/.config/base16-shell"]
	path = base16/.config/base16-shell
	url = https://github.com/chriskempson/base16-shell.git
[submodule "tmux/.tmux/plugins/tpm"]
	path = tmux/.tmux/plugins/tpm
	url = https://github.com/tmux-plugins/tpm
[submodule "zsh/.zsh/spaceship"]
	path = zsh/.zsh/spaceship
	url = https://github.com/spaceship-prompt/spaceship-prompt.git
[submodule "nvm/.nvm"]
	path = nvm/.nvm
	url = https://github.com/nvm-sh/nvm.git
[submodule "zsh/.local/share/zap"]
	path = zsh/.local/share/zap
	url = https://github.com/zap-zsh/zap
